<div id="<%= dom_id product %>" class="card h-100 shadow-sm">
          <img src="<%= product.product_image %>" class="card-img-top" alt="<%= product.product_name %>">
          <div class="card-body">
            <h5 class="card-title"><%= product.product_name %></h5>
            <p class="card-text"><%= product.description %></p>
            <p class="card-text"><strong>$<%= product.price %></strong></P>
            <%= link_to "Add to Cart", cart_items_path, method: "post",  class: "btn btn-primary ", product: product%>
        </div>
      </div>

